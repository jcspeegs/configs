# This file  contains shell agnostic aliases
# For shell specific aliases use: .bashrc_aliases, ...
alias ll='ls -lrta'

alias sls='screen -ls'

alias qc='vim ~/.config/qtile/config.py'
alias qa='vim ~/.config/qtile/scripts/autostart.sh'

alias sb='source ~/.bashrc'

alias cfg='/usr/bin/git --git-dir=/home/ugflows/.dotfiles/ --work-tree=/home/ugflows'
alias cfgs='cfg status'
